<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
		<typeAlias type="com.security.gurume365.vo.Users" alias="Users"/>
		<typeAlias type="com.security.gurume365.vo.Member" alias="Member"/>
		<typeAlias type="com.security.gurume365.vo.FloorLayout" alias="Floor"/>
		<typeAlias type="com.security.gurume365.vo.FloorTable" alias="Tables"/>
	</typeAliases>
    <mappers>
    	<mapper resource="usersMapper.xml"/>
    	<mapper resource="floorMapper.xml"/>
    </mappers>
</configuration>